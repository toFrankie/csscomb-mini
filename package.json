  "devDependencies": {
    "csscomb": "^4.3.0",
    "gulp": "^4.0.2",
    "gulp-csscomb": "^3.1.0",
    "gulp-debug": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "minimist": "^1.2.5",
    "plugin-error": "^1.0.1",
    "through2": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.wxss": [
      "gulp csscombTask --path"
    ]
  }
}
